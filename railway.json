{
  "services": [
    {
      "name": "fastapi-backend",
      "rootDir": "backend",
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port 8000"
    },
    {
      "name": "streamlit-frontend",
      "rootDir": "frontend",
      "startCommand": "streamlit run streamlit_app.py --server.port 80 --server.address 0.0.0.0"
    }
  ]
}

